        <activity
            android:name="org.opencv.samples.facedetect.Fd4Activity"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="landscape" />
        <activity android:name=".ManagerActivity" />
        <activity android:name=".AddStudentActivity" />
        <activity android:name=".AddStudent2Activity" />
        <activity android:name=".ShowStudentActivity" />
        <activity android:name=".StudentSearch" />
        <activity android:name=".CameraOpenActivity" />
        <activity android:name=".ShowStudnetActivity2" />
        <activity android:name=".Test"></activity>
